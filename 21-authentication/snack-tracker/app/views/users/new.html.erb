<h1>Make a new user or <%= link_to "login", login_path %></h1>


<%= render partial: "shared/error_list", locals: {new_object: @user} %>

<%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
    <%= f.submit %>
<% end %>